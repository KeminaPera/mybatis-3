<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.keminapera.mybatis.mapper.StudentMapper">
  <insert id="insertStudent" parameterType="com.keminapera.pojo.Student">
    insert into student (id, name, age) values (#{id}, #{name}, #{age})
  </insert>

  <select id="selectAll" resultType="com.keminapera.pojo.Student">
    select id, name, age from student;
  </select>

  <select id="selectOne" resultType="com.keminapera.pojo.Student">
    select * from student where id=#{id}
  </select>
</mapper>
